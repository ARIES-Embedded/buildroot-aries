From 6fe780cba914f82dfa6e6bc23643c1154b728b15 Mon Sep 17 00:00:00 2001
From: Wolfgang Grandegger <wg@aries-embedded.de>
Date: Fri, 5 May 2023 20:23:59 +0200
Subject: [PATCH 3/3] fdts: stm32mp1: introduce Aries Embedded MSMP1 Module on
 MSMP1EVK

---
 fdts/stm32mp15-bl32.dtsi                |   2 +-
 fdts/stm32mp157a-msmp1evk-fw-config.dts |   7 +
 fdts/stm32mp157a-msmp1evk.dts           | 346 ++++++++++++++++++++++++
 3 files changed, 354 insertions(+), 1 deletion(-)
 create mode 100644 fdts/stm32mp157a-msmp1evk-fw-config.dts
 create mode 100644 fdts/stm32mp157a-msmp1evk.dts

diff --git a/fdts/stm32mp15-bl32.dtsi b/fdts/stm32mp15-bl32.dtsi
index ca4bb3ea5..f33e0c6fe 100644
--- a/fdts/stm32mp15-bl32.dtsi
+++ b/fdts/stm32mp15-bl32.dtsi
@@ -38,7 +38,7 @@
 			/delete-node/ sdmmc2-b4-1;
 			/delete-node/ sdmmc2-d47-0;
 			/delete-node/ sdmmc2-d47-1;
-			/delete-node/ sdmmc2-d47-3;
+			/delete-node/ sdmmc2-d47-4;
 			/delete-node/ usbotg_hs-0;
 			/delete-node/ usbotg-fs-dp-dm-0;
 		};
diff --git a/fdts/stm32mp157a-msmp1evk-fw-config.dts b/fdts/stm32mp157a-msmp1evk-fw-config.dts
new file mode 100644
index 000000000..2abbe50e6
--- /dev/null
+++ b/fdts/stm32mp157a-msmp1evk-fw-config.dts
@@ -0,0 +1,7 @@
+// SPDX-License-Identifier: (GPL-2.0+ OR BSD-3-Clause)
